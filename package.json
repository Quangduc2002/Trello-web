{
  "name": "e-source-code-react-js-trello-web",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run build && vite preview",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "analyze": "npm run lint && tsc && vite build --mode=analyze && source-map-explorer 'dist/assets/*.js'",
    "verify-commit": "verify-commit-msg",
    "postinstall": "git-scm-hooks",
    "release": "bumpp -r",
    "lint": "eslint src",
    "check": "prettier --check src",
    "fix": "eslint --fix src && prettier --write src",
    "gen-icon": "csvgtocss -c icon",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/lodash": "^4.17.14",
    "ahooks": "^3.8.4",
    "antd": "^5.23.0",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "csvgtocss": "^0.1.1",
    "esno": "^4.8.0",
    "jotai": "^2.11.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "sonner": "^1.7.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@hunghg255/eslint-config": "^0.0.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.17.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "bumpp": "^9.8.1",
    "eslint": "^9.16.0",
    "git-scm-hooks": "^0.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.82.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.5",
    "verify-commit-msg": "^0.0.11",
    "vite": "^4.5.5",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-lightningcss": "^0.0.5"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
